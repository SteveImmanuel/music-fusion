{
    "python.analysis.autoImportCompletions": false,
    "python.analysis.typeCheckingMode": "off",
    "[python]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "eeyore.yapf"
    },
    "yapf.args": [
        "--style",
        "{indent_width: 4, column_limit: 130}"
    ],
    "editor.rulers": [
        130
    ],
}